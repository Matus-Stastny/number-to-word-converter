{
    "name": "number-to-word-converter",
    "license": "MIT",
    "version": "0.1.0",
    "description": "Simple React-Node app for converting numbers to words in style of T9",
    "scripts": {
        "client": "cd client && yarn start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
    },
    "dependencies": {
        "express": "^4.16.2"
    },
    "devDependencies": {
        "concurrently": "^3.5.0",
        "nodemon": "^1.18.3"
    }
}
